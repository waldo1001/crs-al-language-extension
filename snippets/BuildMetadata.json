        "body": [
            "<?xml version=\"1.0\" encoding=\"utf-8\"?>",
            "<ExportedData>",
            "\t<TenantWebServiceCollection>",
            "\t\t<TenantWebService>",
            "\t\t\t<ObjectType>${1|Codeunit,Page,Query|}</ObjectType>",
