{
    "Snippet: Trigger": {
        "prefix": "ttriggerwaldo",
        "body": [
            "trigger ${1:OnWhat}()",
            "begin",
            "\t$0",
            "end;"
        ],
        "description": "Snippet: Trigger"
    },
