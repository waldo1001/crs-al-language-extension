    "Snippet: Assert Error": {
        "prefix": "tassertwaldo",
        "body": [
            "assertError ${1:AssertStatement};",
            "if ${2:getLastErrorText} <> ${3:ErrorText} then",
            "\terror('${4:Unexpected error: %1}', ${0:getLastErrorText});"
        ],
        "description": "Snippet: Assert Error"
    },
